{
  "name": "LMS-Lite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ilpd": "npm install --legacy-peer-deps",
    "hmr": "ng serve --configuration hmr",
    "ng-high-memory-serve": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve",
    "ng-high-memory-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=prod",
    "ng-high-memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng build",
    "start": "ng serve  --no-live-reload",
    "buildDev": "ng build --base-href ./ --output-path ./cordova-build/www",
    "buildProd": "ng build --prod --base-href ./ --output-path ./cordova-build/www",
    "build:dev": "ng build --base-href ./ --aot=true --optimization=true --source-map=false --output-path ./cordova-build/www",
    "build-android": "ng build --base-href ./ --aot=true --optimization=true --source-map=false --prod --output-path ./cordova-build/www && npm run start-cordova -- prod",
    "build-android:dev": "ng build --base-href ./ --aot=true --optimization=true --source-map=false --prod --output-path ./cordova-build/www && npm run start-cordova -- dev",
    "start-cordova": "cd ../cordova-lms/lms-lite && npm start"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@agm/markerclusterer": "^3.0.0-beta.0",
    "@angular/animations": "^12.1.0",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.0.4",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@google/markerclustererplus": "^5.1.3",
    "d3": "^3.5.17",
    "ng-zorro-antd": "^12.1.0",
    "ng-zorro-antd-mobile": "^5.0.1",
    "ng2-nvd3": "^2.0.0",
    "ngx-socket-io": "^3.0.1",
    "nvd3": "^1.8.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.19.1",
    "@zxing/ngx-scanner": "^3.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
